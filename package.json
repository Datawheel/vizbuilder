{
  "name": "@datawheel/vizbuilder",
  "version": "0.2.8",
  "description": "A React component that visualizes multiple kinds of charts according to the data from a olap-client query.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "readme": "README.md",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js --environment NODE_ENV:production,BABEL_ENV:production",
    "prepublishOnly": "npm run build",
    "start": "snowpack dev",
    "watch": "rollup --config rollup.config.js --environment NODE_ENV:development,BABEL_ENV:development --watch"
  },
  "keywords": [
    "charts",
    "d3",
    "d3plus",
    "datawheel",
    "mondrian-rest",
    "olap-client",
    "tesseract-olap"
  ],
  "author": "Francisco Abarzua <francisco@datawheel.us>",
  "license": "MIT",
  "homepage": "https://github.com/Datawheel/vizbuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/Datawheel/vizbuilder.git"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.0.0",
    "@datawheel/olap-client": "^2.0.0-beta.3",
    "@datawheel/use-translation": "^0.1.3",
    "classnames": "^2.0.0",
    "d3plus-common": "^1.0.0",
    "d3plus-export": "^1.0.0",
    "d3plus-format": "^1.0.0",
    "d3plus-react": "^1.0.0",
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@datawheel/eslint-config": "^1.0.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@snowpack/plugin-react-refresh": "^2.4.2",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "http-proxy": "^1.18.1",
    "prop-types": "^15.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-inspector": "^5.1.1",
    "rollup": "^2.45.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "snowpack": "^3.3.5"
  },
  "peerDependencies": {
    "prop-types": "^15.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "eslintConfig": {
    "extends": "@datawheel",
    "ignorePatterns": [
      "**/dist/*",
      "**/node_modules/*"
    ]
  }
}
